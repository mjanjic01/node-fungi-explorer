  "scripts": {
    "lint": "tslint 'src/**/*.ts'",
    "typeorm": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm",
    "migration:create": "yarn typeorm migration:create",
    "migration:generate": "yarn typeorm migration:generate",
    "migration:run": "yarn typeorm migration:run",
    "build:client": "NODE_ENV='production' webpack --mode=production",
    "build": "tsc",
    "dev": "NODE_ENV='development' nodemon",
    "start": "NODE_ENV='production' node dist/main",
    "test": "NODE_ENV='test' mocha --delay -r ts-node/register -s 400 --reporter spec --globals __core-js_shared__,typeormMetadataArgsStorage --exit --check-leaks ./test/**/*.spec.ts",
    "coverage": "nyc yarn test",
    "coverage:report": "nyc --reporter text-lcov yarn test && node -r dotenv/config node_modules/codeclimate-test-reporter/bin/codeclimate.js < coverage/lcov.info",
    "precommit": "yarn lint"
  },
